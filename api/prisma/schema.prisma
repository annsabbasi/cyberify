generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Submission {
  id        String   @id @default(uuid())
  email     String   @unique
  name      String
  message   String
  createdAt DateTime @default(now())
}
